{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "^(http|fs|path|reflect-metadata)(/.*)?$",
    "^(supertest|uuid|dotenv|typeorm)",
    "^@/framework/(.*)$",
    "^@/constants(/.*)?$",
    "^@/users/(.*)$",
    "^[./]",
    ".*"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "printWidth": 100
}
